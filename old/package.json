{
  "name": "rld-maptool",
  "version": "0.6.0",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron-build": "ng build --base-href && electron dist",
    "electron": "electron ./src/electron/electron.dev",
    "build": "ng build && node scripts/bundle.js",
    "build-windows": "npm run build && cross-var electron-packager dist/rld-maptool maptool-$npm_package_version --out=build --platform=win32 --arch=all --overwrite",
    "build-linux": "npm run build && cross-var electron-packager dist/rld-maptool maptool-$npm_package_version --out=build --platform=linux --arch=all --overwrite",
    "deploy-windows": "npm run build-windows && node scripts/deploy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "core-js": "^3.8.1",
    "electron": "^11.1.1",
    "esm": "^3.2.25",
    "hammerjs": "^2.0.8",
    "install": "^0.13.0",
    "npm": "^7.11.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.3.0",
    "archiver": "^4.0.2",
    "concurrently": "^6.4.0",
    "cross-var": "^1.1.0",
    "electron-packager": "^15.2.0",
    "ts-node": "~8.10.2",
    "typescript": "~4.5.2"
  }
}
